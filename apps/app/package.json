{
  "name": "app",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "start": "pnpm run dev",
    "dev": "run-p dev:frontend dev:backend",
    "dev:frontend": "vite --port 3000",
    "dev:backend": "convex dev",
    "setup": "convex dev --run init --until-success",
    "build": "tsc -b && vite build"
  },
  "dependencies": {
    "@auth/core": "0.37.0",
    "@aws-sdk/client-ses": "^3.826.0",
    "@convex-dev/auth": "^0.0.87",
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@fontsource-variable/geist": "^5.2.6",
    "@fontsource-variable/geist-mono": "^5.2.6",
    "@oslojs/crypto": "^1.0.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-form": "^1.12.2",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.66.5",
    "@tanstack/react-router": "^1.114.3",
    "@tanstack/react-router-devtools": "^1.114.3",
    "@tanstack/router-plugin": "^1.114.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.19.2",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.476.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@tanstack/eslint-config": "^0.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
